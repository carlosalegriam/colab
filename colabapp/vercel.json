{
  "version": 2,
  "builds": [
    {
      "src": "colabapp/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "colabapp/wsgi.py"
    }
  ],
  "env": {
    "VERCEL_BUILD_CMD": "python manage.py collectstatic --noinput"
  }
}
